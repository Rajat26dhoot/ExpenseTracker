{
  "expo": {
    "name": "ExpenseTrackerApp",
    "slug": "ExpenseTrackerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashImage.png",
    "scheme": "expensetrackerapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashImage.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rajat26.ExpenseTrackerApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splashImage.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashImage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "507da05b-e3fe-4cb7-bd31-03d5c5bb338b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/507da05b-e3fe-4cb7-bd31-03d5c5bb338b"
    }
  }
}
